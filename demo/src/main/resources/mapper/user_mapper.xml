<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	<mapper namespace="com.example.demo.dao.UserDAO">
		<select id="testUser"
			resultType="com.example.demo.dto.User">
			SELECT * FROM users
		</select>

		<insert id="signUpInfo" parameterType="com.example.demo.dto.User" >
			INSERT INTO
			users (user_id, nickname, password1, password2)
			VALUES
			(#{user_id}, #{nickname}, #{password1}, #{password2});
		</insert>
	</mapper>